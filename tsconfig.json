{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/atoms/*": [
        "./components/atoms/*"
      ],
      "@/molecules/*": [
        "./components/molecules/*"
      ],
      "@/organisms/*": [
        "./components/organisms/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}